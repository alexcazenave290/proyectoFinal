{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
          { "source": "/login", "destination": "/login.html" }
        ]
}
